{
  "name": "ajincodew-academy",
  "version": "1.0.0-RC1",
  "private": true,
  "scripts": {
    "start": "node watch-and-restart.js",
    "dev": "npm-run-all --parallel dev:*",
    "build": "npm-run-all --serial build:*",
    "dev:11ty": "eleventy --serve",
    "dev:css": "npx tailwindcss -i src/assets/css/tailwind.css -o dist/assets/css/tailwind.css --watch --postcss",
    "build:11ty": "eleventy",
    "build:css": "npx tailwindcss -i src/assets/css/tailwind.css -o dist/assets/css/tailwind.css --postcss --minify",
    "clean": "git clean -e node_modules -x -d -i"
  },
  "dependencies": {
    "@11ty/eleventy": "^3.0.0",
    "@11ty/eleventy-img": "^6.0.3",
    "@heroicons/react": "^2.2.0",
    "@material-tailwind/react": "^2.1.10",
    "@ryangjchandler/alpine-mask": "^0.2.0",
    "@tailwindcss/forms": "^0.5.10",
    "alpinejs": "^3.14.9",
    "autoprefixer": "^10.4.21",
    "axios": "^1.9.0",
    "cleave.js": "^1.6.0",
    "cssnano": "^7.0.7",
    "eleventy-hast-jsx": "^0.3.5",
    "eleventy-plugin-i18n": "^0.1.3",
    "esbuild": "^0.25.4",
    "flowbite": "^3.1.2",
    "lodash": "^4.17.21",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.5.3",
    "postcss-import": "^16.1.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "swiper": "^11.2.6",
    "tailwindcss": "^4.1.6",
    "tailwindcss-dir": "^4.0.0"
  },
  "devDependencies": {
    "@types/alpinejs": "^3.13.11",
    "chokidar": "^4.0.3",
    "daisyui": "^5.0.35",
    "rimraf": "^6.0.1",
    "tree-kill": "^1.2.2"
  }
}